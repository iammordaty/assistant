        "fig/http-message-util": "^1.1",
        "guzzlehttp/guzzle": "^7.7",
        "iammordaty/key-tools": "^0.8.2",
        "iammordaty/musly-php": "^0.5.2",
        "james-heinrich/getid3": "^1.9",
        "kevinrob/guzzle-cache-middleware": "^4.1",
        "khill/php-duration": "^1.1",
        "mongodb/mongodb": "^1.11",
        "monolog/monolog": "^2.4",
        "pagerfanta/core": "^4.1",
        "pagerfanta/twig": "^4.1",
        "php-di/php-di": "^6.3",
        "php-di/slim-bridge": "^3.2",
        "slim/http": "^1.3",
        "slim/slim": "^4.11",
        "slim/twig-view": "^3.3",
